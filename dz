for i in range(2, 23, 2):
    print (i)
print("--------------------")
for i in range(11, 100, 11):
    print(i)
print("--------------------")
for i in range(1, 11, 1):
    print(5,"*", i, "=", 5*i)
print("--------------------")
for i in range(1, 11, 1):
    print(i, "КГ цукерок коштує", i*2.5, "ГРН")
